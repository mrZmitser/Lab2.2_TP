<hibernate-mapping>
    <class name="Shop" table="shop">
    <id column="id" name="id" type="java.lang.Long">
    <generator class="increment"/>
</id>
<property column="shop_name" name="shop_name" type="java.lang.String"/>
<property column="shop_description" name="shop_description" type="java.lang.String"/>

<set name="buyers" lazy="false">
<key column="shop_id"/>
<one-to-many class="Buyer"/>
        </set>

        </class>
        </hibernate-mapping>